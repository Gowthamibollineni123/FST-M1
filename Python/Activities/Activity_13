# Custom function to calculate sum
def calculate_sum(numbers):
	sum = 0
	for number in numbers:
		sum += number
	return sum

numList = [10, 40, 60, 90]
result = calculate_sum(numList)
print("The sum of all the elements is: " + str(result))

